{
  "name": "@viclafouch/eslint-config-viclafouch",
  "version": "4.22.1-beta.14",
  "description": "ESLint and Prettier Config from Victor de la Fouchardiere",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.ts",
  "bin": {
    "eslint-config-viclafouch": "./bin/init.js"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs"
    }
  },
  "keywords": [
    "javascript",
    "ecmascript",
    "eslintconfig",
    "typescript",
    "eslint",
    "config",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/eslint-config-viclafouch.git"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "author": "Victor de la Fouchardiere <victor.delafouchardiere@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/viclafouch/eslint-config-viclafouch#readme",
  "peerDependencies": {
    "eslint": ">= 9",
    "prettier": ">= 3",
    "typescript": ">= 5"
  },
  "dependencies": {
    "@eslint/compat": "^2.0.2",
    "@next/eslint-plugin-next": "^16.1.6",
    "@total-typescript/ts-reset": "^0.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.3.0",
    "prettier-plugin-curly": "^0.4.1",
    "typescript-eslint": "^8.54.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "bump": "npm version minor",
    "publish": "npm publish",
    "bump:beta": "npm version prerelease --preid beta",
    "publish:beta": "npm publish --tag beta"
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "typescript": "^5.7.2"
  }
}
