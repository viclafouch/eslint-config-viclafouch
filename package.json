{
  "name": "@viclafouch/eslint-config-viclafouch",
  "version": "4.22.0",
  "description": "ESLint and Prettier Config from Victor de la Fouchardiere",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs"
    }
  },
  "keywords": [
    "javascript",
    "ecmascript",
    "eslintconfig",
    "typescript",
    "eslint",
    "config",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/eslint-config-viclafouch.git"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "author": "Victor de la Fouchardiere <victor.delafouchardiere@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/viclafouch/eslint-config-viclafouch#readme",
  "peerDependencies": {
    "eslint": ">= 9",
    "prettier": ">= 3",
    "typescript": ">= 5"
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@eslint/js": "^9.21.0",
    "@next/eslint-plugin-next": "^15.2.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "app-root-path": "^3.1.0",
    "babel-loader": "^10.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "get-tsconfig": "^4.10.0",
    "globals": "^16.0.0",
    "prettier": ">= 3",
    "prettier-plugin-curly": "^0.3.1",
    "typescript": ">= 5",
    "typescript-eslint": "^8.26.0",
    "eslint": ">= 9"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "bump": "npm version minor",
    "publish": "npm publish",
    "bump:beta": "npm version prerelease --preid beta",
    "publish:beta": "npm publish --tag beta"
